#include <stdio.h>

const int n=15;
int tablero1, tablero2;
main ()
{
	for(tablero1=0;tablero1<n;tablero1++)
	{
		for(tablero2=0;tablero2<n;tablero2++)
		{
			if(tablero1==n-1 || tablero1==0 && (tablero2%2)==0 || tablero1==7 ||  tablero1==0 && tablero2==1 ||   tablero1==0 && tablero2==13 )
			{
				printf("%c ", 219);
			}
			else
			{
			printf("%c ", 46);				
			}
			
		}
		printf("\n");
	}		
	
}

